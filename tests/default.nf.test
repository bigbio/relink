nextflow_pipeline {

    name "Test Workflow RELINK"
    script "../main.nf"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
        }

        then {
            assert workflow.success
            assert snapshot(
                path("$outputDir/pipeline_info/nf_core_pipeline_software_mqc_versions.yml")
            ).match("versions")
        }

    }

}

